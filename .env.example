# AirController local release environment template
# Copy this file to .env and replace placeholders before signed release builds.

# Android signing inputs (required for signed APK/AAB)
ANDROID_SIGNING_STORE_FILE=/absolute/path/to/release.keystore
ANDROID_SIGNING_STORE_PASSWORD=replace_me
ANDROID_SIGNING_KEY_ALIAS=replace_me
ANDROID_SIGNING_KEY_PASSWORD=replace_me

# Optional: Base64 of keystore (used in CI workflows)
ANDROID_KEYSTORE_BASE64=replace_me
